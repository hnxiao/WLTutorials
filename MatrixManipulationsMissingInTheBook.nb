Cell["\<\
Suppose constraints are given by Mx<=b and x>=0. Put it into Mx<=b form first.\
\>", "Text",
 CellChangeTimes->{{3.623146502695409*^9, 3.623146519936315*^9}, {
  3.623147672763633*^9, 3.623147683700197*^9}, {3.62314781504445*^9, 
  3.6231478242528763`*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"d", "=", 
   RowBox[{
    RowBox[{"Dimensions", "[", "M", "]"}], "[", 
    RowBox[{"[", "2", "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"b", "=", 
   RowBox[{"Join", "[", 
    RowBox[{"b", ",", 
     RowBox[{"ConstantArray", "[", 
      RowBox[{"0", ",", "d"}], "]"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"M", "=", 
   RowBox[{"Join", "[", 
    RowBox[{"M", ",", 
     RowBox[{"-", 
      RowBox[{"IdentityMatrix", "[", "d", "]"}]}]}], "]"}]}], ";"}]}], "Input",\

 CellChangeTimes->{{3.6231470125572653`*^9, 3.6231470521982317`*^9}, {
  3.623147088924748*^9, 3.623147141238399*^9}, {3.623147539641801*^9, 
  3.62314763623341*^9}, {3.6231478304969673`*^9, 3.6231478380146427`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"cddwstpVE", "[", 
   RowBox[{"M", ",", "b"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"b", "=", 
      RowBox[{"List", "/@", "b"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"A", "=", 
      RowBox[{"ArrayFlatten", "@", 
       RowBox[{"{", 
        RowBox[{"{", 
         RowBox[{"b", ",", 
          RowBox[{"-", "M"}]}], "}"}], "}"}]}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{
      RowBox[{"{", 
       RowBox[{"m", ",", "d1"}], "}"}], "=", 
      RowBox[{"Dimensions", "[", "A", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"vertices", ",", "linearity"}], "}"}], ",", "incidences"}], 
       "}"}], "=", 
      RowBox[{"AllVertices", "[", 
       RowBox[{"m", ",", "d1", ",", 
        RowBox[{"ToString", "[", 
         RowBox[{"Flatten", "[", "A", "]"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"vlist", "=", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Drop", "[", 
          RowBox[{"#", ",", "1"}], "]"}], "&"}], ",", " ", 
        RowBox[{"ToExpression", "[", "vertices", "]"}]}], "]"}]}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.623146706095786*^9, 3.6231467383156357`*^9}, {
